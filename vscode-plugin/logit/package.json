{
	"name": "logit",
	"publisher": "ruderngespra",
	"displayName": "logit",
	"description": "Logit-js writes informative console logs into javascript files.",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.logit"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.logit",
				"title": "logit"
			},
			{
				"command": "extension.logit-verbose",
				"title": "logit-verbose"
			},
			{
				"command": "extension.logit-remove",
				"title": "logit-remove"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ruderngespra/logit-js/tree/master/vscode-plugin/logit"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.7.2",
		"@types/vscode": "^1.37.0",
		"eslint": "^6.2.1",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.5.3",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"logit-js": "^0.2"
	}
}
